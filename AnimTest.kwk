<project id="AnimTest">  <settings>    <name>AnimTest</name>    <media>Book</media>    <device>New iPad</device>    <OS>iOS</OS>    <zoom>zoomStretch</zoom>    <width>2048</width>    <height>1536</height>    <orientation>Landscape</orientation>    <resolution>264</resolution>    <bookmark>false</bookmark>    <swipe>true</swipe>    <pinch>false</pinch>    <thumbs>false</thumbs>  </settings>  <pages>    <page id="page1">      <name>page1</name>      <number>1</number>      <file>~/Documents/Kwik/AnimTest2/page1.psd</file>      <fileName>page1.psd</fileName>      <anim id="linear_pressed">        <name>linear_pressed</name>        <target>Layer</target>        <type>Linear</type>        <layer>mizumanju</layer>        <endX>847</endX>        <endY>1000</endY>        <endTarget>Fixed</endTarget>        <endAlpha>100</endAlpha>        <duration>0.3</duration>        <delay>0</delay>        <easing>Linear</easing>        <pause>false</pause>        <linearLoop>1</linearLoop>        <linearReverse>true</linearReverse>        <rotate>0</rotate>        <anchor>BottomCenter</anchor>        <scaleW>110</scaleW>        <scaleH>95</scaleH>        <event>Wait request</event>        <reStart>false</reStart>        <flipX>false</flipX>        <flipY>false</flipY>        <audio>None</audio>        <trigger>None</trigger>        <breadCrumb>false</breadCrumb>        <breadAnchor>Center</breadAnchor>        <breadShape>Rectangle</breadShape>        <breadWidth>10</breadWidth>        <breadHeight>10</breadHeight>        <breadColor>255, 255, 255</breadColor>        <breadInterval>50</breadInterval>        <breadDispose>true</breadDispose>        <breadSec>1</breadSec>        <groupControl>false</groupControl>      </anim>      <anim id="linear_falling">        <name>linear_falling</name>        <target>Layer</target>        <type>Linear</type>        <layer>mizumanju</layer>        <endX>847</endX>        <endY>1000</endY>        <endTarget>Fixed</endTarget>        <endAlpha>100</endAlpha>        <duration>2</duration>        <delay>0</delay>        <easing>Linear</easing>        <pause>false</pause>        <linearLoop>1</linearLoop>        <linearReverse>false</linearReverse>        <rotate>0</rotate>        <anchor>BottomCenter</anchor>        <scaleW>100</scaleW>        <scaleH>100</scaleH>        <event>Wait request</event>        <reStart>false</reStart>        <flipX>false</flipX>        <flipY>false</flipY>        <audio>None</audio>        <trigger>act_afterfallin</trigger>        <breadCrumb>false</breadCrumb>        <breadAnchor>Center</breadAnchor>        <breadShape>Rectangle</breadShape>        <breadWidth>10</breadWidth>        <breadHeight>10</breadHeight>        <breadColor>255, 255, 255</breadColor>        <breadInterval>50</breadInterval>        <breadDispose>true</breadDispose>        <breadSec>1</breadSec>        <groupControl>false</groupControl>      </anim>      <button id="but_startanim">        <name>but_startanim</name>        <layer>sceneGroup</layer>        <rollover>mizumanju</rollover>        <target>Page</target>        <type>Tap</type>        <tap>1</tap>        <action id="anim_945">          <name>anim_945</name>          <type>Play Animation</type>          <animation>linear_falling</animation>        </action>      </button>      <trigger id="act_afterfallin">        <name>act_afterfallin</name>        <action id="anim_973">          <name>anim_973</name>          <type>Play Animation</type>          <animation>linear_pressed</animation>        </action>      </trigger>    </page>  </pages></project>